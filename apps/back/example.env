DATABASE_URL=postgresql://postgres:postgres@localhost:5432/verseau2

# SANDRE Mock Configuration
USE_SANDRE_MOCK=false
# Optional: Configure mock behavior
# SANDRE_MOCK_BEHAVIOR=conformant
# SANDRE_MOCK_CONFORMANT_RATE=0.5

S3_PROVIDER=outscale #ou mock
S3_BUCKET=MY_BUCKET
S3_ENDPOINT=OUTSCALE_ENDPOINT
S3_REGION=OUTSCALE_REGION
S3_ACCESS_KEY=MY_ACCESS_KEY
S3_SECRET_KEY=MY_SECRET_KEY

LOGS_LEVEL=debug # or verbose

# Email
EMAIL_PROVIDER=mock # or brevo or #mailcatcher

# OpenID Connect
OIDC_ISSUER_URL=https://your-oidc-provider/.well-known/openid-configuration
OIDC_CLIENT_ID=your-client-id
OIDC_MOCK=false