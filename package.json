{
  "name": "verseau2",
  "private": false,
  "scripts": {
    "build": "pnpm --filter back build",
    "start": "pnpm --filter back start",
    "dev": "concurrently \"pnpm dev:back\" \"pnpm dev:front\"",
    "dev:back": "pnpm --filter back dev",
    "dev:front": "pnpm --filter front dev",
    "prepare": "husky",
    "clean": "pnpm --filter back clean && pnpm --filter front clean && rm -rf node_modules"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "typescript": "^5.7.3"
  },
  "version": "0.0.0"
}
