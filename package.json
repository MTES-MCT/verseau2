{
  "name": "verseau2",
  "private": false,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspace=apps/back",
    "start": "npm run start --workspace=apps/back",
    "dev": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "dev:back": "npm run dev --workspace=apps/back",
    "dev:front": "npm run dev --workspace=apps/front",
    "prepare": "husky"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "husky": "^9.1.7"
  },
  "dependencies": {}
}
